<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1100.0" xmlns:fx="http://javafx.com/fxml" fx:controller="simulator.Controller">
  <children>
    <Button layoutX="1000.0" layoutY="30.0" mnemonicParsing="false" onAction="#onExploreClick" text="EXPLORE" />
    <Button layoutX="1000.0" layoutY="60.0" mnemonicParsing="false" onAction="#onStartClick" text="START" />
    <Button layoutX="1000.0" layoutY="90.0" mnemonicParsing="false" onAction="#onReconnect" text="RESET" />
    <RadioButton layoutX="1000.0" layoutY="122.0" mnemonicParsing="false" selected="true" text="Speed 1" onAction="#setSpeed1" />
    <RadioButton layoutX="1000.0" layoutY="145.0" mnemonicParsing="false" text="Speed 2" onAction="#setSpeed2" />
    <RadioButton layoutX="1000.0" layoutY="167.0" mnemonicParsing="false" text="Speed 3" onAction="#setSpeed3" />
    <RadioButton layoutX="1000.0" layoutY="189.0" mnemonicParsing="false" text="Speed 4" onAction="#setSpeed4" />
    <RadioButton layoutX="1000.0" layoutY="211.0" mnemonicParsing="false" text="Speed 5" onAction="#setSpeed5" />
    <!--
    <Button layoutX="1000.0" layoutY="30" mnemonicParsing="false" text="MOVE" onAction="#onMoveClick" />
    <Button layoutX="1000.0" layoutY="60" mnemonicParsing="false" text="STOP" onAction="#onStopClick" />
    <Button layoutX="1000.0" layoutY="90" mnemonicParsing="false" text="LEFT" onAction="#onLeftClick" />
    <Button layoutX="1000.0" layoutY="120" mnemonicParsing="false" text="RIGHT" onAction="#onRightClick" />
    <Button layoutX="1000.0" layoutY="150" mnemonicParsing="false" text="AHEAD" onAction="#onAheadClick" />
    -->
  </children>
</AnchorPane>
